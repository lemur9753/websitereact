{classes.listitem}>\r\n          <ListItemIcon className={classes.icon}>\r\n            <FontAwesomeIcon icon={faTimes} size=\"2x\"/>\r\n          </ListItemIcon>\r\n        </ListItem>\r\n      </List>\r\n        <Divider/>\r\n      <List className={classes.drawer}>\r\n        {itemsList.map((item, index) => {\r\n          const { text, icon, location} = item;\r\n          return (\r\n            <ListItem button key={text} onClick={() => onSelect(location,text)} className={classes.listitem} selected={selected===text}>\r\n              {icon && <ListItemIcon className={classes.icon}>{icon}</ListItemIcon>}\r\n              <ListItemText primary={text} />\r\n            </ListItem>\r\n          );\r\n        })}\r\n      </List>\r\n      <Divider/>\r\n      <div className={classes.bottomPush}>\r\n        <List className={classes.drawer}>\r\n          <ListItem button key={'Adminbereich'} onClick={() => onSelect('/admin','Adminbereich')} className={classes.listitem} selected={selected==='Adminbereich'}>\r\n              <ListItemIcon className={classes.icon}>\r\n                <FontAwesomeIcon icon={faSignInAlt} size=\"2x\"/>\r\n              </ListItemIcon>\r\n              <ListItemText primary={'Upload'} />\r\n            </ListItem>\r\n        </List>\r\n      </div>\r\n    </MUIDrawer>\r\n  );\r\n};\r\n\r\nexport default withRouter(Sidebar)","E:\\Website\\website\\test\\src\\uploadblog.js",[],"E:\\Website\\website\\test\\src\\downloadblog.js",[],"E:\\Website\\website\\test\\src\\bilder\\bilder.js",[],{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","severity":1,"message":"60","line":71,"column":5,"nodeType":"61","endLine":71,"endColumn":7,"suggestions":"62"},"no-native-reassign",["63"],"no-negated-in-lhs",["64"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'history.location.pathname'. Either include it or remove the dependency array.","ArrayExpression",["65"],"no-global-assign","no-unsafe-negatio